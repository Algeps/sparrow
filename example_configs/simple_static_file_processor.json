{
  "workers": [
    {
      "name": "simple static file processor",
      "port": 8080,
      "protocolConfig": {
        "protocol": "HTTP_1_1"
      },
      "filtersConfig": [],
      "handlersConfig": [
        {
          "handlerType": "HTTP_STATIC_CONTENT",
          "path": "/**",
          "httpMethods": [
            "GET"
          ],
          "dirs": [
            "./"
          ]
        }
      ]
    }
  ]
}

